public:
    // arr: input array
    // n: size of array
    //Function to find the sum of contiguous subarray with maximum sum.
    long long maxSubarraySum(int arr[], int n){
        
        // Your code here
        int MAX=INT_MIN;
        int sum=0;
        for(int i=0;i<n;i++){
            sum=sum+arr[i];
            MAX=max(sum,MAX);
            if(sum<0){
                sum=0;
            }
        }
        return MAX;
        
    }
